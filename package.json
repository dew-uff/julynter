{
  "name": "julynter",
  "version": "0.1.0",
  "description": "A quality checker for Jupyter",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/dew-uff/julynter",
  "bugs": {
    "url": "https://github.com/dew-uff/julynter/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Joao Felipe Pimentel",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/dew-uff/julynter.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "prepare": "jlpm run clean && jlpm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.0.0",
    "@jupyterlab/docmanager": "^2.0.0",
    "@jupyterlab/fileeditor": "^2.0.0",
    "@jupyterlab/notebook": "^2.0.0",
    "@jupyterlab/nbformat": "^2.0.0",
    "@lumino/disposable": "^1.4.0",
    "react": "~16.9.0",
    "react-dom": "~16.9.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2", 
    "@types/react": "~16.9.0",
    "@types/react-dom": "~16.9.0",
    "prettier": "1.16.4",
    "rimraf": "^2.6.1",
    "typescript": "~3.7.0"
  },
  "resolutions": {
    "@types/react": "~16.9.0"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true
  }
}
